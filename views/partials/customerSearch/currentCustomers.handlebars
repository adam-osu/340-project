<div id="current-customers" class="current-customers">
  <input type="hidden" id="current-property-max-occupancy" value="0" />
  <table>
    <thead>
      <th>Name</th>
      <th>Email</th>
      <th></th>
    </thead>
    {{#each this}}
      <tr>
        <td style="display: none"><input type="hidden" value {{this.id}} /></td>
        <td>{{this.first_name}} {{this.last_name}}</td>
        <td>{{this.email}}</td>
        <td><button class="btn--small">Remove</button></td>
      </tr>
    {{/each}}
    <tbody>

    </tbody>
  </table>
</div>