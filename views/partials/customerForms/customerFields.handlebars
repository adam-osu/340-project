<div>
    <label for="first_name">First Name</label>
    <input type="text" id="first_name" name="first_name" {{#if first_name}}value={{first_name}}{{/if}}>
</div>
<div>
    <label for="last_name">Last Name</label>
    <input type="text" id="last_name" name="last_name" {{#if last_name}}value={{last_name}}{{/if}}>
</div>
<div>
    <label for="email">Rate</label>
    <input type="email" id="email" name="email" {{#if email}}value={{email}}{{/if}}>
</div>
<button>Submit</button>