<div>
    <label for="building_name">Property Name</label>
    <input type="text" id="building_name" name="building_name" {{#if this.building_name}}value={{this.building_name}}{{/if}}>
</div>
<div>
    <label for="address">Address</label>
    <input type="text" id="address" name="address" {{#if address}}value={{address}}{{/if}}>
</div>
<div>
    <label for="rate">Rate</label>
    <input type="number" id="rate" name="rate" {{#if rate}}value={{rate}}{{/if}}>
</div>
<div>
    <label for="max_occupancy">Max Occupancy</label>
    <input type="number" id="max_occupancy" name="max_occupancy" {{#if max_occupancy}}value={{max_occupancy}}{{/if}}>
</div>
<button>Submit</button>