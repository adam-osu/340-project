<div>
    <label for="name">Property Name</label>
    <input type="text" id="name" name="name" {{#if propertyName}}value={{propertyName}}{{/if}}>
</div>
<div>
    <label for="name">Address</label>
    <input type="text" id="address" name="address" {{#if address}}value={{address}}{{/if}}>
</div>
<div>
    <label for="rate">Rate</label>
    <input type="number" id="rate" name="rate" {{#if rate}}value={{rate}}{{/if}}>
</div>
<div>
    <label for="rate">Max Occupancy</label>
    <input type="number" id="max-occupancy" name="max-occupancy" {{#if maxOccupancy}}value={{maxOccupancy}}{{/if}}>
</div>
<button>Submit</button>